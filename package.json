{
  "name": "patientapppushnotification",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:ios": "expo run:ios && expo start",
    "start:ios-clear": "expo run:ios && expo start -c",
    "start:android": "expo run:android && expo start",
    "start:android-clear": "expo run:android && expo start -c",
    "start:dev-cli-ios": "expo run:ios --no-build-cache && expo start --dev-client",
    "start:dev-cli-android": "expo run:android --no-build-cache && expo start --dev-client",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "npx expo prebuild",
    "check": "npx expo install --check",
    "doctor": "npx expo-doctor",
    "clean-packages": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf ./ios/Podfile.lock"
  },
  "dependencies": {
    "@react-native-firebase/app": "^22.4.0",
    "@react-native-firebase/messaging": "^22.4.0",
    "@tanstack/query-sync-storage-persister": "^5.83.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-persist-client": "^5.83.0",
    "axios": "^1.11.0",
    "dotenv": "^16.6.1",
    "expo": "~53.0.20",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-linking": "~7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "google-auth-library": "^10.2.0",
    "mitt": "^3.0.1",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^3.3.0",
    "react-native-reanimated": "^4.0.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-worklets": "^0.4.1",
    "react-query-kit": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
